{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "baseUrl": ".",
  "paths": {
    "*": ["src/*"],
    "tests": ["tests/*"],
    "@assets/*": [
      "./src/assets/*"
    ],
    "@components/*": [
      "./src/components/*"
    ],
    "@screens/*": [
      "./src/screens/*"
    ],
    "@routes/*": [
      "./src/routes/*"
    ]
  },
}
